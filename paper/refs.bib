	journal = {Journal of Economic Theory},
	pages = {1291-1299},
	title = {Risk-Neutral Firms Can Extract Unbounded Profits from Consumers with Prospect Theory Preferences},
	url = {https://doi.org/10.1016/j.jet.2012.01.002},
